<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AddWidget">
        <attr name="circle_anim" format="boolean"/>
        <attr name="sub_anim" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="UnderlineDevider">
        <attr name="devide_count" format="integer" />
        <attr name="devide_color" format="color"/>
    </declare-styleable>
    <declare-styleable name="ItemView">
        <attr name="left_icon" format="integer"/>
        <attr name="show_left_icon" format="boolean"/>
        <attr name="left_text" format="string"/>
        <attr name="right_text" format="string"/>
        <attr name="show_right_arrow" format="boolean"/>
        <attr name="show_bottom_line" format="boolean"/>
    </declare-styleable>
</resources>